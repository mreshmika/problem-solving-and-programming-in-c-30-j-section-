#include <stdio.h>
int main()
{
    int a,b,c;
    printf("enter the length of first side of a triangle");
    scanf("%d",&a);
    printf("enter the length of 2nd side of a triangle");
    scanf("%d",&b);
    printf("enter the length of 3rd side of a triangle");
    scanf("%d",&c);
    if (a==b==c)
    {
    printf("the triangle is equilateral");
    }
    else if (a==b||b==c||c==a)
    {
        printf("the triangle is isoseles");
    }
    else
    {
        printf("the triangle is scalen");
    }
    return 0;
}
