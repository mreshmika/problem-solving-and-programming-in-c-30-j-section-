#include<stdio.h>
int main ()
{
  int a, b, c;
  printf ("enter the values of a,b and c:");
  scanf ("%d %d %d", &a, &b, &c);
  if (a < b & a < c)
    {
      printf("%d is least",a);
    }
  else if (b < a & b < c)
    {
      printf("%d is least",b);
    }
  else
  {
    printf("%d is least",c);
  }
  return 0;
}
